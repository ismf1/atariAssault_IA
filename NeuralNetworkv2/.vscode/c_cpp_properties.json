{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}", "${workspaceFolder}/include", 
            "${workspaceFolder}/../guardar"
        ]
    },
    "configurations": [
        {
            "name": "Mac",
            "intelliSenseMode": "clang-x64",
            "includePath": [
                "${myDefaultIncludePath}"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "browse": {
                "path": ["${workspaceFolder}"],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}
